            android:orientation="horizontal"
            android:background="@color/bluegray_101"

            >


            <View android:background="@color/gray_401"
                android:id="@+id/lineLineFive"
                android:layout_height="@dimen/_142pxv"
                android:layout_marginTop="@dimen/_3pxv"
                android:layout_weight="0.0" android:layout_width="0dp"/>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearColumnthigianly"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_2pxv"
                android:layout_marginStart="@dimen/_18pxh"
