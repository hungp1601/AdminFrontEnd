                    tools:text="@string/msg_th_i_gian_l_y"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtPlate"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
