                <TextView
                    android:id="@+id/txtThigiangi"
                    style="@style/txtInterregular15_2"
                    android:layout_width="371dp"
                    android:layout_height="wrap_content"
                    android:singleLine="true"
                    android:text=""
                    tools:text="@string/msg_th_i_gian_g_i" />

                <TextView android:ellipsize="end"
                    android:id="@+id/txtThigianly"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:singleLine="true"
                    android:text=""
